{
  "name": "arraycache",
  "version": "1.0.0",
  "license": "ISC",
  "author": "Sebastian Scholle",
  "description": "store and manage caches or array data",
  "keywords": [
    "array",
    "cache",
    "expireable",
    "localstorage",
    "helper"
  ],
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.3",
    "typescript": "^4.3.5"
  },
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "example": "npm run build && node ./examples/microwave.js"
  },
  "files": [
    "lib"
  ],
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "^.+\\.spec\\.ts$"
  }
}
